{
	"ligações": [
		{
			"administrador": true,
			"único": false,
			"de": [
				"./configuração/global/profile",
				"./configuração/global/environment",
				"./configuração/global/locale.conf"
			],
			"para": "/etc/"
		},
		{
			"administrador": true,
			"único": false,
			"de": "./subconfiguração/ícones/",
			"para": "/usr/share/icons/"
		},
		{
			"administrador": true,
			"único": false,
			"de": "./serviços/global/*",
			"para": "/run/runit/service/"
		},
		{
			"administrador": false,
			"único": false,
			"de": "./serviços/local/*",
			"para": "~/.local/services/"
		},
		{
			"administrador": false,
			"único": false,
			"de": "./configuração/local/",
			"para": "~/.config/"
		},
		{
			"administrador": false,
			"único": true,
			"de": "./subconfiguração/fontes",
			"para": "~/.local/share/fonts"
		},
		{
			"administrador": false,
			"único": true,
			"de": "./subconfiguração/nushell",
			"para": "~/.local/share/nupm"
		},
		{
			"administrador": false,
			"único": true,
			"de": "./subconfiguração/clipboard",
			"para": "~/.cache/cliphist"
		},
		{
			"administrador": false,
			"único": false,
			"de": "./subconfiguração/aplicativos/",
			"para": "~/.local/share/applications/"
		}
	]
}
