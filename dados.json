{
	"ligações": {
		"administrador": [
			{
				"único": false,
				"de": [
					"./configuração/global/profile",
					"./configuração/global/environment",
					"./configuração/global/locale.conf"
				],
				"para": "/etc/"
			},
			{
				"único": false,
				"de": "./subconfiguração/ícones/",
				"para": "/usr/share/icons/"
			},
			{
				"único": false,
				"de": {
					"pasta": "./serviços/global/*",
					"exclusão": "supervise"
				},
				"para": "/run/runit/service/"
			}
		],
		"local": [
			{
				"único": false,
				"de": {
					"pasta": "./serviços/local/*",
					"exclusão": "supervise"
				},
				"para": "~/.local/services/"
			},
			{
				"único": false,
				"de": "./configuração/local/",
				"para": "~/.config/"
			},
			{
				"único": true,
				"de": "./subconfiguração/fontes",
				"para": "~/.local/share/fonts"
			},
			{
				"único": true,
				"de": "./subconfiguração/nushell",
				"para": "~/.local/share/nupm"
			},
			{
				"único": true,
				"de": "./subconfiguração/clipboard",
				"para": "~/.cache/cliphist"
			},
			{
				"único": false,
				"de": "./subconfiguração/aplicativos/",
				"para": "~/.local/share/applications/"
			}
		]
	}
}
